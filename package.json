{
  "name": "@bigmistqke/solid-contenteditable",
  "version": "0.0.12",
  "description": "textual contenteditable build with solid-js",
  "license": "MIT",
  "author": "bigmistqke",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigmistqke/solid-contenteditable.git"
  },
  "homepage": "https://github.com/bigmistqke/solid-contenteditable#readme",
  "bugs": {
    "url": "https://github.com/bigmistqke/solid-contenteditable/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "build:dev": "vite build dev",
    "build": "tsup",
    "dev": "vite serve dev",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "generate-supported-events": "pnpm test:e2e --project=chromium --reporter=json -- w3c-input-types | node script/support.ts",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "lint": "concurrently pnpm:lint:*",
    "prepublishOnly": "pnpm build",
    "test": "playwright test -c test/playwright.config.ts",
    "test:json": "playwright test -c test/playwright.config.ts --reporter=json",
    "test:ui": "pnpm test -- --ui",
    "test:serve": "vite serve test",
    "update-deps": "pnpm up -Li",
    "release": "pnpm bumpp && pnpm publish"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "bumpp": "^10.3.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.3",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^24.0.0",
    "prettier": "3.0.0",
    "solid-js": "^1.8.17",
    "tsup": "^8.0.2",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.4.5",
    "valibot": "^1.1.0",
    "vite": "^5.2.11",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "solid",
    "contenteditable"
  ],
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3"
  }
}
